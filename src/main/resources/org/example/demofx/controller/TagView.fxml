<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<HBox fx:id="TagContainer" alignment="CENTER" onMouseClicked="#handleTagClick" spacing="5" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.demofx.controller.TagController">
    <Circle fx:id="tagIcon" centerY="5.0" fill="DODGERBLUE" radius="5.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0">
        <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </HBox.margin>
    </Circle>
    <Label fx:id="tagName" prefHeight="41.0" prefWidth="83.0" text="TagName">
        <font>
            <Font size="16.0" />
        </font>
    </Label>
</HBox>
